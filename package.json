{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@yuikigai/yuikigai",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "pnpm -- turbo run build --concurrency=4",
		"build:affected": "turbo run build --filter='...[origin/main]' --concurrency=4",
		"test": "turbo run test --concurrency=4",
		"lint": "turbo run lint --concurrency=4",
		"lint:affected": "turbo run lint --filter='...[origin/main]' --concurrency=4",
		"format": "turbo run format --concurrency=4",
		"fmt": "turbo run format --concurrency=4",
		"dev": "turbo run dev --filter=!@yuikigai/website --concurrency=4",
		"prepare": "is-ci || husky install"
	},
	"type": "module",
	"contributors": [
		"Nicolas Ribeiro <git@niskii.dev>",
		"Davi Patricio <davipatricio@pm.me>"
	],
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/yuikigai/yuikigai.git"
	},
	"bugs": {
		"url": "https://github.com/yuikigai/yuikigai/issues"
	},
	"devDependencies": {
		"@types/lodash.merge": "^4.6.7",
		"eslint": "^8.51.0",
		"eslint-config-neon": "^0.1.57",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lodash.merge": "^4.6.2",
		"prettier": "^3.0.3",
		"tsup": "^7.2.0",
		"turbo": "^1.10.15",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"engines": {
		"node": ">=18.17.1"
	},
	"packageManager": "pnpm@8.9.0"
}
