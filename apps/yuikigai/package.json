{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@yuikigai/yuikigai",
	"version": "0.1.0",
	"description": "",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"dev": "tsx --watch src/index.ts",
		"deploy-dev": "node dist/deploy/deploy-dev.js",
		"deploy-global": "node dist/deploy/deploy-global.js",
		"start": "node dist/index.js",
		"lint": "prettier --check . && eslint --format=pretty src",
		"format": "prettier --write . && eslint --fix --format=pretty src"
	},
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"directories": {
		"lib": "src"
	},
	"files": [
		"dist"
	],
	"contributors": [
		"Nicolas Ribeiro <git@niskii.dev>",
		"Davi Patricio <davipatricio@pm.me>"
	],
	"license": "Apache-2.0",
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/yuikigai/yuikigai.git",
		"directory": "apps/yuikigai"
	},
	"bugs": {
		"url": "https://github.com/yuikigai/yuikigai/issues"
	},
	"dependencies": {
		"@yuikigai/framework": "workspace:^",
		"@yuikigai/http": "workspace:^",
		"dotenv": "^16.3.1",
		"readdirp": "^3.6.0",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@types/node": "^20.8.6",
		"eslint": "^8.48.0",
		"eslint-config-neon": "^0.1.57",
		"eslint-formatter-pretty": "^5.0.0",
		"prettier": "^3.0.3",
		"tsx": "^3.13.0",
		"turbo": "^1.10.15",
		"typescript": "^5.2.2"
	},
	"engines": {
		"node": ">=16.11.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
