{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"module": "Node16",
		"moduleResolution": "Node16",
		"resolveJsonModule": true,
		"outDir": "dist",
		"lib": ["ESNext"],
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": ["src/**/*.ts"]
}
