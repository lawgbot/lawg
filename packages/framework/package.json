{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@lawgbot/framework",
	"version": "0.0.1",
	"description": "",
	"scripts": {
		"test": "vitest run",
		"dev": "tsup --watch --clean",
		"build": "tsc --noEmit && tsup",
		"lint": "prettier --check . && eslint --format=pretty src",
		"format": "prettier --write . && eslint --fix --format=pretty src"
	},
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			}
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"directories": {
		"lib": "src",
		"test": "__tests__"
	},
	"files": [
		"dist"
	],
	"contributors": [
		"Nicolas Ribeiro <git@niskii.dev>",
		"Davi Patricio <davipatricio@pm.me>"
	],
	"license": "Apache-2.0",
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/lawgbot/lawg.git",
		"directory": "packages/framework"
	},
	"bugs": {
		"url": "https://github.com/lawgbot/lawg/issues"
	},
	"dependencies": {
		"@lawgbot/structures": "workspace:^",
		"@lawgbot/utils": "workspace:^",
		"discord-api-types": "^0.37.60",
		"pino": "^8.16.0",
		"tsyringe": "^4.8.0"
	},
	"devDependencies": {
		"@prisma/client": "^5.4.2",
		"@types/node": "^20.8.6",
		"eslint": "^8.48.0",
		"eslint-config-neon": "^0.1.57",
		"eslint-formatter-pretty": "^5.0.0",
		"fastify": "^4.24.1",
		"ioredis": "^5.3.2",
		"prettier": "^3.0.3",
		"prisma": "^5.4.2",
		"tsup": "^7.2.0",
		"turbo": "^1.10.15",
		"typescript": "^5.2.2",
		"vitest": "^0.34.6"
	},
	"engines": {
		"node": ">=16.11.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
